{
  "functions": { "source": ".output/server", "runtime": "nodejs20" },
  "hosting": [
    {
      "site": "positivus-lp",
      "public": ".output/public",
      "cleanUrls": true,
      "rewrites": [{ "source": "**", "function": "server" }],
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ]
    }
  ]
}